	юТест = ЮнитТестирование;
	
	ВсеТесты = Новый Массив;
	
	ВсеТесты.Добавить(""Тест_должен_открыть_обработку_vanessa_behavior_и_сгенерировать_epf_по_тестовой_фиче"");
	
	Возврат ВсеТесты;
	
